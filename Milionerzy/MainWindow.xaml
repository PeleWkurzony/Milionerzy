<Window x:Class="Milionerzy.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Milionerzy"
        mc:Ignorable="d"
        Title="Milionerzy" Height="480" Width="800" HorizontalAlignment="Stretch" WindowStartupLocation="Manual">
    <Window.Resources>
        <Style x:Key="GridStyle1" TargetType="{x:Type Grid}"/>
    </Window.Resources>
    <Grid Style="{DynamicResource GridStyle1}">
        <MediaElement LoadedBehavior="Play" UnloadedBehavior="Manual" Name="mp4" Source="./images/background.mp4" MediaEnded="on_media_end" />
        <Grid x:Name="userContent">
            
        </Grid>
    </Grid>
</Window>
