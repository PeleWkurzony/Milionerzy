<Window x:Class="Milionerzy.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Milionerzy"
        mc:Ignorable="d"
        Title="MainWindow" Height="500" Width="800" HorizontalAlignment="Stretch">
    <Window.Resources>
        <Style x:Key="GridStyle1" TargetType="{x:Type Grid}"/>
    </Window.Resources>
    <Grid Style="{DynamicResource GridStyle1}">
        <Grid.Background>
            <ImageBrush Stretch="UniformToFill" ImageSource="/images/background.jpg"/>
        </Grid.Background>
        <Grid x:Name="userContent">
            
        </Grid>
    </Grid>
</Window>
