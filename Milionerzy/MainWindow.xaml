            <Button x:Name="ui_exit4" Content="Wyjdź z gry" HorizontalAlignment="Center" Margin="0,384,0,0" VerticalAlignment="Top" Height="40" Width="180" Background="#00DDDDDD"/>
            <Grid HorizontalAlignment="Center" Margin="0,329,0,0" VerticalAlignment="Top" Width="179">
                <Button x:Name="ui_credits4" Content="Autorzy" HorizontalAlignment="Left" VerticalAlignment="Top" Height="40" Width="116" Background="#00DDDDDD"/>
                <Button x:Name="ui_credits_Copy4" HorizontalAlignment="Left" VerticalAlignment="Center" Height="40" Width="52" Background="#00DDDDDD" Margin="127,0,0,0">
