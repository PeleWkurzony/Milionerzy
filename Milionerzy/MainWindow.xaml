                <Button x:Name="ui_credits_Copy1" HorizontalAlignment="Left" VerticalAlignment="Center" Height="40" Width="52" Background="#00DDDDDD" Margin="127,0,0,0">
                    <Image Source="/setting.png"/>
                </Button>
            </Grid>
        </Grid>
        <Grid x:Name="form_find_game" Style="{DynamicResource GridStyle1}" d:IsHidden="True">
            <Label x:Name="Logo2" Content="Jeden z Dziesięciu" HorizontalAlignment="Center" Margin="0,29,0,0" VerticalAlignment="Top" Height="140" Width="460" FontSize="48" FontWeight="Bold" FontStyle="Italic" FontStretch="Normal" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
